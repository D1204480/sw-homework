<assembly>
	<includeBaseDirectory>true</includeBaseDirectory>
	<baseDirectory>Baralga-Multiuser-${project.version}</baseDirectory>
	<formats>
		<format>zip</format>
		<format>dir</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>.</directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>Licence.txt</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/multiuser</directory>
			<outputDirectory>data</outputDirectory>
			<includes>
				<include>application.properties</include>
			</includes>
		</fileSet>
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory></outputDirectory>
			<outputFileNameMapping>
				Baralga-${artifact.version}.${artifact.extension}
			</outputFileNameMapping>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<fileMode>0777</fileMode>
			<includes>
				<include>com.remast.baralga:baralga-core</include>
			</includes>
		</dependencySet>
	</dependencySets>
</assembly>
